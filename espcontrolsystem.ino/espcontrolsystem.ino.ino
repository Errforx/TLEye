// HTTPS server and cert
SSLCert *cert;
HTTPSServer *server;

// Siren blink control (keep your originals)
bool siren_active = false;
unsigned long last_blink = 0;
bool blink_state = false;

// Handler for /set_mode?mode=xxx
void handleSetMode(HTTPRequest *req, HTTPResponse *res) {
  // Check for "mode" parameter
  std::string mode = req->getParams()->get("mode");

  if (!mode.empty()) {
    // Reset siren if not siren mode
    if (mode != "siren_on") {
      siren_active = false;
    }

    if (mode == "red") {
      startTimer(30);
      digitalWrite(LED_RED, HIGH);
      digitalWrite(LED_YELLOW, LOW);
      digitalWrite(LED_GREEN, LOW);
      digitalWrite(sLED_Green, HIGH);
      digitalWrite(sLED_Yellow, LOW);
      digitalWrite(sLED_Red, LOW);
    } else if (mode == "green") {
      startTimer(30);
      digitalWrite(LED_RED, LOW);
      digitalWrite(LED_YELLOW, LOW);
      digitalWrite(LED_GREEN, HIGH);
      digitalWrite(sLED_Green, LOW);
      digitalWrite(sLED_Yellow, LOW);
      digitalWrite(sLED_Red, HIGH);
    } else if (mode == "yellow") {
      startTimer(3);
      digitalWrite(LED_RED, LOW);
      digitalWrite(LED_YELLOW, HIGH);
      digitalWrite(LED_GREEN, LOW);
      digitalWrite(sLED_Green, LOW);
      digitalWrite(sLED_Yellow, HIGH);
      digitalWrite(sLED_Red, LOW);
    } else if (mode == "siren_on") {
      siren_active = true;
      startTimer(10);
      last_blink = millis();
      blink_state = false;  // Start with off
    } else if (mode == "killall") {
      digitalWrite(LED_RED, LOW);
      digitalWrite(LED_YELLOW, LOW);
      digitalWrite(LED_GREEN, LOW);
      digitalWrite(sLED_Green, LOW);
      digitalWrite(sLED_Yellow, LOW);
      digitalWrite(sLED_Red, LOW);
    }

    res->setStatusCode(200);
    res->setStatusText("OK");
    res->println("OK");
  } else {
    res->setStatusCode(400);
    res->setStatusText("Missing mode parameter");
    res->println("Missing mode parameter");
  }
}

void setup() {
  // Initialize LEDs (keep your originals)
  pinMode(LED_GREEN, OUTPUT);
  pinMode(LED_YELLOW, OUTPUT);
  pinMode(LED_RED, OUTPUT);
  digitalWrite(LED_GREEN, LOW);
  digitalWrite(LED_YELLOW, LOW);
  digitalWrite(LED_RED, LOW);

  pinMode(sLED_Green, OUTPUT);
  pinMode(sLED_Yellow, OUTPUT);
  pinMode(sLED_Red, OUTPUT);
  digitalWrite(sLED_Green, LOW);
  digitalWrite(sLED_Yellow, LOW);
  digitalWrite(sLED_Red, LOW);

  // Serial for debugging
  Serial.begin(115200);

  // Start in AP mode (keep your originals)
  WiFi.mode(WIFI_AP);
  WiFi.softAP(ap_ssid, ap_password);
  
  // Set static IP for ESP32 in AP mode
  IPAddress apIP(192, 168, 4, 1);
  IPAddress gateway(192, 168, 4, 1);
  IPAddress subnet(255, 255, 255, 0);
  WiFi.softAPConfig(apIP, gateway, subnet);

  Serial.println();
  Serial.print("Access Point started! SSID: ");
  Serial.println(ap_ssid);
  Serial.print("IP address: ");
  Serial.println(WiFi.softAPIP());  // Should print 192.168.4.1

  // Setup 7-segment display (keep your originals)
  setupDisplay();

  // Use existing DER-encoded cert and key
  Serial.println("Loading existing certificate...");

  cert = new SSLCert(
    x509, sizeof(x509),  // DER cert
    rsakey, sizeof(rsakey)  // DER key
  );

  // Initialize HTTPS server with the cert
  server = new HTTPSServer(cert);

  // Register the /set_mode handler (GET method)
  ResourceNode *nodeSetMode = new ResourceNode("/set_mode", "GET", &handleSetMode);
  server->registerNode(nodeSetMode);

  // Start server
  Serial.println("Starting HTTPS server...");
  server->start();
  if (server->isRunning()) {
    Serial.println("HTTPS server ready on https://192.168.4.1:443");
  }
}

void loop() {
  updateTimerDisplay();
  server->loop();  // Handle HTTPS requests (non-blocking)

  // Handle siren blinking if active (keep your originals)
  if (siren_active) {
    unsigned long now = millis();
    if (now - last_blink >= (blink_state ? 1000 : 400)) {
      blink_state = !blink_state;
      if (blink_state) {
        digitalWrite(LED_RED, HIGH);
        digitalWrite(LED_YELLOW, HIGH);
        digitalWrite(LED_GREEN, HIGH);
        digitalWrite(sLED_Green, LOW);
        digitalWrite(sLED_Yellow, LOW);
        digitalWrite(sLED_Red, HIGH);
      } else {
        digitalWrite(LED_RED, LOW);
        digitalWrite(LED_YELLOW, LOW);
        digitalWrite(LED_GREEN, LOW);
        digitalWrite(sLED_Green, LOW);
        digitalWrite(sLED_Yellow, LOW);
        digitalWrite(sLED_Red, LOW);
      }
      last_blink = now;
    }
  }
}

// Keep your original functions for display and timer
void setupDisplay() {
  pinMode(SEG_A, OUTPUT);
  pinMode(SEG_B, OUTPUT);
  pinMode(SEG_C, OUTPUT);
  pinMode(SEG_D, OUTPUT);
  pinMode(SEG_E, OUTPUT);
  pinMode(SEG_F, OUTPUT);
  pinMode(SEG_G, OUTPUT);

  pinMode(DIGIT_1, OUTPUT);
  pinMode(DIGIT_2, OUTPUT);

  digitalWrite(DIGIT_1, LOW);
  digitalWrite(DIGIT_2, LOW);
}

void displayDigit(int digit, int digitPin) {
  digitalWrite(DIGIT_1, LOW);
  digitalWrite(DIGIT_2, LOW);

  digitalWrite(SEG_A, digitMap[digit][0]);
  digitalWrite(SEG_B, digitMap[digit][1]);
  digitalWrite(SEG_C, digitMap[digit][2]);
  digitalWrite(SEG_D, digitMap[digit][3]);
  digitalWrite(SEG_E, digitMap[digit][4]);
  digitalWrite(SEG_F, digitMap[digit][5]);
  digitalWrite(SEG_G, digitMap[digit][6]);

  digitalWrite(digitPin, HIGH);
}

void startTimer(int seconds) {
  segtimerDuration = seconds * 1000UL;
  segtimerStart = millis();
  segtimerRunning = true;
}

void updateTimerDisplay() {
  if (!segtimerRunning) return;

  unsigned long elapsed = millis() - segtimerStart;

  if (elapsed >= segtimerDuration) {
    segtimerRunning = false;
    digitalWrite(DIGIT_1, LOW);
    digitalWrite(DIGIT_2, LOW);
    return;
  }

  int remaining = (segtimerDuration - elapsed) / 1000;
  int tens = remaining / 10;
  int ones = remaining % 10;

  if (millis() - seglastRefresh > 3) {
    seglastRefresh = millis();
    segtoggleDigit = !segtoggleDigit;

    if (segtoggleDigit)
      displayDigit(tens, DIGIT_1);
    else
      displayDigit(ones, DIGIT_2);
  }
}